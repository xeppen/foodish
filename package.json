{
  "name": "foodish",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm clean:next-dev && next dev",
    "clean:next": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "clean:next-dev": "node -e \"require('fs').rmSync('.next-dev',{recursive:true,force:true})\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:unit",
    "test:unit": "vitest run --exclude \"**/*.integration.test.ts\"",
    "test:integration": "vitest run integration.test.ts",
    "test:coverage": "vitest run --coverage --exclude \"**/*.integration.test.ts\"",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@clerk/localizations": "^3.35.3",
    "@clerk/nextjs": "^6.37.3",
    "@prisma/client": "5.22.0",
    "lucide-react": "^0.563.0",
    "next": "^15.5.12",
    "react": "18",
    "react-dom": "18",
    "uploadthing": "^7.7.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.24",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.12",
    "postcss": "^8",
    "prisma": "5.22.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
